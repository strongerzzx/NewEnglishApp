<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CutDownView">
        <!--外面圆的颜色-->
        <attr name="outColor" format="color"/>

        <!--外圆的宽-->
        <attr name="outCircleWidth" format="dimension"/>

        <!--内圆的颜色-->
        <attr name="innerColor" format="color"/>

        <!--内圆的宽-->
        <attr name="innerCircleWidth" format="dimension"/>

        <!--text颜色-->
        <attr name="textColor" format="color"/>

        <!--大小-->
        <attr name="textWidth" format="dimension"/>
    </declare-styleable>



    <!--涟漪控件属性-->
    <declare-styleable name="mRippleView">
        <attr name="cColor" format="color"/>
        <attr name="cSpeed" format="integer"/>
        <attr name="cDensity" format="integer"/>
        <attr name="cIsFill" format="boolean"/>
        <attr name="cIsAlpha" format="boolean"/>
    </declare-styleable>
</resources>